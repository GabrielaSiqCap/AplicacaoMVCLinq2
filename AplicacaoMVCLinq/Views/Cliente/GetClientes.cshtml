@model IEnumerable<AplicacaoMVCLinq.Models.Cliente>

@{
    ViewData["Title"] = "Listar Clientes";
}

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
            <th>Data Nascimento</th>
        </tr>
    </thead>

    <!--Laço de repetição foreach para listar tdodos os elementos da lista aluno-->
    <tbody>
        @foreach (var cliente in Model)
        {
            <tr>
                <td>@cliente.Id</td>
                <td>@cliente.Nome</td>
                <td>@cliente.Email</td>
                <td>@cliente.Telefone</td>
                <td>@cliente.DataNasc</td>
            </tr>
        }
    </tbody>
</table>
